{
  "name": "turboshots",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev --parallel",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\""
  },
  "devDependencies": {
    "@turbo/gen": "^1.10.13",
    "eslint": "^8.49.0",
    "eslint-config-custom": "workspace:*",
    "prettier": "^3.0.3",
    "prettier-plugin-tailwindcss": "^0.5.4",
    "tsconfig": "workspace:*",
    "turbo": "^1.10.13"
  },
  "packageManager": "pnpm@8.6.10"
}
